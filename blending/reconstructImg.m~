function resultImg = reconstructImg(indexes, red, green, blue, targetImg)

[row col] = size(indexes);

red = reshape(red,[row col]);
blue = reshape(blue,[row col]);
green = reshape(green,[row col]);

resultImg = zeros(row,col,3);
result(:,:,1) = red;
result(:,:,2) = blue;
result(:,:,3)

end